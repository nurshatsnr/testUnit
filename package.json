    "testomatio": "cd test && TESTOMATIO=tstmt_mOpdRYr9iIY7AO3c7WtvlkGmYGYG65L6pA1712079177 npx newman run --folder RF-AVM -d qalist2.csv  ITB-206.postman_collection.json -r testomatio,cli,progress,junit,htmlextra",
